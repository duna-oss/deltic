{
  "name": "@deltic/stack",
  "type": "module",
  "version": "0.0.0",
  "description": "Combining the deltic components into a cohesive unit",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "module-sync": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory.js",
      "require": "./dist/memory.cjs",
      "module-sync": "./dist/memory.js",
      "types": "./dist/memory.d.ts"
    },
    "./pg": {
      "import": "./dist/pg.js",
      "require": "./dist/pg.cjs",
      "module-sync": "./dist/pg.js",
      "types": "./dist/pg.d.ts"
    },
    "./rabbitmq": {
      "import": "./dist/rabbitmq.js",
      "require": "./dist/rabbitmq.cjs",
      "module-sync": "./dist/rabbitmq.js",
      "types": "./dist/rabbitmq.d.ts"
    },
    "./outbox-relay": {
      "import": "./dist/outbox-relay.js",
      "require": "./dist/outbox-relay.cjs",
      "module-sync": "./dist/outbox-relay.js",
      "types": "./dist/outbox-relay.d.ts"
    }
  },
  "keywords": ["event-sourcing", "stack", "infrastructure", "deltic", "rabbitmq", "outbox"],
  "scripts": {},
  "author": "Frank de Jonge (https://frankdejonge.nl)",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@deltic/dependency-injection": "workspace:^",
    "@deltic/event-sourcing": "workspace:^",
    "@deltic/messaging": "workspace:^",
    "@deltic/transaction-manager": "workspace:^"
  },
  "peerDependencies": {
    "@deltic/async-pg-pool": "workspace:^",
    "@deltic/dependency-injection": "workspace:^",
    "@deltic/event-sourcing": "workspace:^",
    "@deltic/messaging": "workspace:^",
    "@deltic/mutex": "workspace:^",
    "@deltic/transaction-manager": "workspace:^",
    "amqplib": "^0.10.7",
    "pg": "^8.16.3"
  },
  "peerDependenciesMeta": {
    "@deltic/async-pg-pool": {
      "optional": true
    },
    "@deltic/mutex": {
      "optional": true
    },
    "amqplib": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "devDependencies": {
    "@deltic/async-pg-pool": "workspace:^",
    "@deltic/mutex": "workspace:^",
    "amqplib": "^0.10.7",
    "@types/amqplib": "^0.10.7",
    "pg": "^8.16.3",
    "@types/pg": "^8.15.6",
    "uuid": "^11.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duna-oss/deltic.git",
    "directory": "packages/stack"
  }
}
