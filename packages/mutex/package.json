      "require": "./dist/memory.cjs",
      "module-sync": "./dist/memory.js",
      "types": "./dist/memory.d.ts"
    },
    "./static-memory": {
      "import": "./dist/static-memory.js",
      "require": "./dist/static-memory.cjs",
      "module-sync": "./dist/static-memory.js",
      "types": "./dist/static-memory.d.ts"
