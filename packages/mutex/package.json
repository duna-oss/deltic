{
  "name": "@deltic/mutex",
  "type": "module",
  "version": "0.0.0",
  "description": "Contract based mutex, allowing key-based or global locks.",
  "files": [
    "package.json",
    "dist"
  ],
  "exports": {
    ".": {
      "import": "dist/index.js",
      "types": "dist/index.d.ts"
    },
    "./memory": {
      "import": "dist/memory-mutex.js",
      "types": "dist/memory-mutex.d.ts"
    },
    "./static-memory": {
      "import": "dist/static-memory-mutex.js",
      "types": "dist/static-memory-mutex.d.ts"
    }
  },
  "keywords": [
    "errors",
    "error"
  ],
  "scripts": {},
  "author": "Frank de Jonge (https://frankdejonge.nl)",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@deltic/abort-signal-options": "workspace:^"
  }
}
