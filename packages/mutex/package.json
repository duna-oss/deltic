{
  "name": "@deltic/mutex",
  "type": "module",
  "version": "0.1.0",
  "description": "Contract based mutex, allowing key-based or global locks.",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "module-sync": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory-mutex.js",
      "require": "./dist/memory-mutex.cjs",
      "module-sync": "./dist/memory-mutex.js",
      "types": "./dist/memory-mutex.d.ts"
    },
    "./static-memory": {
